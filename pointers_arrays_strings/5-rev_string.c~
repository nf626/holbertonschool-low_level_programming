#include "main.h"
#include <stdio.h>

/**
 * print_rev - reverse a string.
 * @s: s variable.
 *
 */
void rev_string(char *s)
{
  int i, j, k;

  while (s[k] != '\0')
    {
      k++;
    }

  j = k - 1;

  while (i < j)
    {
      char c = s[i];
      s[i] = s[j];
      s[j] = c;

      i++;
      j--;
    }
}
